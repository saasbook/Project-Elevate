

<h1><%= "#{@user.name}'s Calendar" %></h1>

<%= month_calendar events: @calendars do |date, calendar| %>
      <%= date%>
    
      <% calendar.each do |calendar| %>
        <div>
          <%= link_to calendar.name, calendar_path(calendar), :class => 'navbar-link' %>
        </div>
      <% end %>
    <% end %>
<br>


<%= link_to "Back", all_calendar_path, :class => 'navbar-link'  %> |
<%= link_to "Back to Profile", root_path, :class => 'navbar-link'  %>


