<p id="notice"><%= notice %></p>

<h1>Calendar</h1>

<%= month_calendar events: @calendars do |date, calendar| %>
      <%= date %>
    
      <% calendar.each do |calendar| %>
        <div>
          <%= calendar.name %>
        </div>
      <% end %>
    <% end %>
<br>




<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Userid</th>
      <th>Otherid</th>
      <th>Start time</th>
      <th>End time</th>
      <th>Typeevent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calendars.each do |calendar| %>
      <tr>
        <td><%= calendar.name %></td>
        <td><%= calendar.UserId %></td>
        <td><%= calendar.OtherId %></td>
        <td><%= calendar.start_time %></td>
        <td><%= calendar.end_time %></td>
        <td><%= calendar.typeEvent %></td>
        <td><%= link_to 'Show', calendar %></td>
        <td><%= link_to 'Edit', edit_calendar_path(calendar) %></td>
        <td><%= link_to 'Destroy', calendar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calendar', new_calendar_path %>
