
<% if notice %>
    <p class="alert alert-success"><%= notice %></p>
<% end %>

<h2 class="text-center">Other's Calendars</h2>

<table class="table table-hover" id="other_calendars">
  <thead>
    <tr>
      <th>User</th>
      <th>Email</th>
      <th>Membership Status</th>
      <th>Calendar</th>
      <th>Event List</th>

    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr id="<%= user.name + '_record'%>">
        <td class="name"><%= user.name %></td>
        <td class="email"><%= user.email %></td>
        <td class="membership"><%= user.membership %></td>
        <td><%= link_to 'View Calendar', viewall_calendar_path(user.id), :class => 'navbar-link', :id => "#{user.name}" + "calendar" %></td>
        <td><%= link_to 'Event List', eventList_calendar_path(user.id), :class => 'navbar-link', :id => "#{user.name}" + "list" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to "Back", member_profile_path, :class => 'btn btn-link'  %>



