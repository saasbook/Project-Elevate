<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Email</th>
      <th>Calendar</th>

    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
