<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Start time</th>
      <th>End time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calendars.each do |calendar| %>
      <tr>
        <td><%= calendar.name %></td>

        <td><%= calendar.start_time %></td>
        <td><%= calendar.end_time %></td>
        <td><%= link_to "Details", calendar_path(calendar), :class => 'navbar-link' %> </td>
         <!--link_to 'Edit', edit_calendar_path(calendar)-->
        <!--link_to 'Delete', calendar, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
      </tr>
    <% end %>
  </tbody>
</table>
<br>

